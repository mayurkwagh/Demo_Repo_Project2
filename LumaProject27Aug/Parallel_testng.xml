<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="methods" name="RegresionSuite">


	<listeners>
		<listener class-name="com.luma.utilities.ListenerSetup" />
		<listener class-name="com.luma.utilities.ExtentReporterNG" />
	</listeners>

	<parameter name="browser" value="chrome" />
